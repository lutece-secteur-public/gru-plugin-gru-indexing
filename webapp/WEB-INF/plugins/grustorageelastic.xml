<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    <!-- Plugin Informations -->
    <name>grustorageelastic</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.2-SNAPSHOT</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>grustorageelastic.module.description</description>
    <provider>grustorageelastic.module.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/grustorageelastic/modules/elastic/gru-elastics.png</icon-url>
    <copyright>Copyright 2001-2015 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>5.0.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>
    <db-pool-required>1</db-pool-required>

 <!-- Admin features parameters -->
    <admin-features>
	 <admin-feature>
            <feature-id>ES_INDEXATION</feature-id>
            <feature-title>grustorageelastic.adminFeature.IndexerES.name</feature-title>
            <feature-description>grustorageelastic.IndexerES.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/grustorageelastic/IndexerES.jsp</feature-url>
            <feature-icon-url/>
        </admin-feature>
     </admin-features>
        
	<filters>
	<!-- REST security : Comment the filters if you want to test the REST service with the given JSP 
       <filter>
           <filter-name>GRUElasticsSecurity</filter-name>
           <url-pattern>/rest/gruElastic/*</url-pattern>
           <filter-class>fr.paris.lutece.util.signrequest.servlet.HeaderHashRequestFilter</filter-class>

           <init-param>
               <param-name>elementsSignature</param-name>
               <param-value>strJson</param-value>
           </init-param>

           <init-param>
               <param-name>validityTimePeriod</param-name>
               <param-value>0</param-value>
           </init-param>

           <init-param>
               <param-name>privateKey</param-name>
               <param-value>gruElastic</param-value>
           </init-param>
       </filter>-->
   </filters>
</plug-in>